{
  "name": "perlcon-bundle",
  "version": "0.0.0",
  "description": "Bundle for PerlCon in Riga",
  "homepage": "https://github.com/shadow-dot-cat/perlcon-bundle",
  "author": {
    "name": "Tom Bloor",
    "email": "tom@tbsliver.me",
    "url": "https://tbsliver.me"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "perlcon",
    "nodecg",
    "obs",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "panel",
        "title": "Panel",
        "width": 2,
        "file": "panel.html",
        "headerColor": "#525F78"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "footer.html",
        "width": 1920,
        "height": 260
      }
    ],
    "assetCategories": [
      {
        "name": "perlcon-corporate-sponsors",
        "title": "PerlCon Corporate Sponsors",
        "allowedTypes": [
          "png",
          "jpeg",
          "jpg",
          "gif"
        ]
      },
      {
        "name": "perlcon-community-sponsors",
        "title": "PerlCon Community Sponsors",
        "allowedTypes": [
          "png",
          "jpeg",
          "jpg",
          "gif"
        ]
      }
    ]
  },
  "repository": "https://github.com/shadow-dot-cat/perlcon-bundle.git",
  "license": "MIT",
  "dependencies": {
    "bower-npm-resolver": "^0.10.0"
  }
}
