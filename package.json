{
  "name": "perlcon-bundle",
  "version": "0.0.0",
  "description": "Bundle for PerlCon in Riga",
  "homepage": "https://github.com/shadow-dot-cat/perlcon-bundle",
  "author": {
    "name": "Tom Bloor",
    "email": "tom@tbsliver.me",
    "url": "https://tbsliver.me"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "perlcon",
    "nodecg",
    "obs",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "master",
        "title": "Master",
        "width": 2,
        "file": "master.html",
        "headerColor": "#525F78"
      },
      {
        "name": "room_1_speakers",
        "title": "Room 1 Speakers",
        "width": 4,
        "workspace": "Room 1",
        "file": "room_1_speakers.html",
        "headerColor": "#525F78"
      },
      {
        "name": "room_2_speakers",
        "title": "Room 2 Speakers",
        "width": 4,
        "workspace": "Room 2",
        "file": "room_2_speakers.html",
        "headerColor": "#525F78"
      },
      {
        "name": "room_3_speakers",
        "title": "Room 3 Speakers",
        "width": 4,
        "workspace": "Room 3",
        "file": "room_3_speakers.html",
        "headerColor": "#525F78"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "footer.html",
        "width": 1920,
        "height": 260
      },
      {
        "file": "room_banner.html",
        "width": 1920,
        "height": 820
      },
      {
        "file": "room_1_sidebar.html",
        "width": 1920,
        "height": 820
      },
      {
        "file": "room_1_speaker.html",
        "width": 1920,
        "height": 820
      },
      {
        "file": "room_2_sidebar.html",
        "width": 1920,
        "height": 820
      },
      {
        "file": "room_2_speaker.html",
        "width": 1920,
        "height": 820
      },
      {
        "file": "room_3_sidebar.html",
        "width": 1920,
        "height": 820
      },
      {
        "file": "room_3_speaker.html",
        "width": 1920,
        "height": 820
      }
    ],
    "assetCategories": [
      {
        "name": "perlcon-major-sponsors",
        "title": "PerlCon Major Sponsors",
        "allowedTypes": [
          "png",
          "jpeg",
          "jpg",
          "gif"
        ]
      },
      {
        "name": "perlcon-minor-sponsors",
        "title": "PerlCon Minor Sponsors",
        "allowedTypes": [
          "png",
          "jpeg",
          "jpg",
          "gif"
        ]
      }
    ]
  },
  "repository": "https://github.com/shadow-dot-cat/perlcon-bundle.git",
  "license": "MIT",
  "dependencies": {
    "bower-npm-resolver": "^0.10.0"
  }
}
